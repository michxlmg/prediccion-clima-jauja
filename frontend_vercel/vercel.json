{
  "framework": null,
  "buildCommand": null,
  "outputDirectory": ".",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://huggingface.co"
        },
        {
          "key": "X-Frame-Options",
          "value": "ALLOW-FROM https://huggingface.co"
        }
      ]
    }
  ]
} 